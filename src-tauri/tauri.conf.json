{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "eros-vibe",
  "version": "0.1.0",
  "identifier": "org.studio26f.eros-vibe",
  "build": {
    "frontendDist": "../dist/spa",
    "devUrl": "http://localhost:9200",
    "beforeDevCommand": "quasar dev",
    "beforeBuildCommand": "quasar build"
  },
  "app": {
    "windows": [
      {
        "title": "Eros Vibe",
        "width": 1200,
        "height": 900,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
